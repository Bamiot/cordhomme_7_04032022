{
  "semi": false,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "printWidth": 80,
  "overrides": [
    {
      "files": ["*.svg", "*.html"],
      "options": {
        "parser": "html"
      }
    },
    {
      "files": ["*.scss", "*.css"],
      "options": {
        "parser": "css"
      }
    }
  ]
}
